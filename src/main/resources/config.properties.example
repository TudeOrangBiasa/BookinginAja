# ============================================
# Hotel Management System - Configuration
# ============================================
# Copy this file to config.properties and fill in your values

# Supabase PostgreSQL Configuration
# Get these from: Supabase Dashboard > Settings > Database
db.host=db.YOUR_PROJECT_REF.supabase.co
db.port=5432
db.name=postgres
db.username=postgres
db.password=YOUR_SUPABASE_PASSWORD

# Connection Pool Settings
db.pool.size=5
db.pool.timeout=30000

# Application Settings
app.name=Hotel Management System
app.version=1.0.0
